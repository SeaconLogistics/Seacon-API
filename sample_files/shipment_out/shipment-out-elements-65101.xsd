<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.kewill.com/logistics/klic/shipment-out" elementFormDefault="qualified" targetNamespace="http://www.kewill.com/logistics/klic/shipment-out">
  <xs:annotation>
    <xs:documentation>Base type and element specifications</xs:documentation>
  </xs:annotation>
  <xs:simpleType name="NType">
    <xs:annotation>
      <xs:documentation>Non-negative integer value (no fractional digits).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:nonNegativeInteger"/>
  </xs:simpleType>
  <xs:simpleType name="AType">
    <xs:annotation>
      <xs:documentation>Character string value without control characters.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(\P{Cc})*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DTType">
    <xs:annotation>
      <xs:documentation>Date value specified in the following form "YYYY-MM-DDThh:mm:ss".</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:dateTime"/>
  </xs:simpleType>
  <xs:simpleType name="stringType">
    <xs:annotation>
      <xs:documentation>Character string value</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="DYType">
    <xs:annotation>
      <xs:documentation>Date value specified in the following form "YYYY-MM-DD".</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date"/>
  </xs:simpleType>
  <xs:simpleType name="TSType">
    <xs:annotation>
      <xs:documentation>Time value specified in the following form "hh:mm:ss".</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:time"/>
  </xs:simpleType>
  <xs:simpleType name="AUType">
    <xs:annotation>
      <xs:documentation>Character string value (uppercase) without control characters.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:pattern value="([^a-z])*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="booleanType">
    <xs:annotation>
      <xs:documentation>Boolean type to specify a true or false value.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:boolean"/>
  </xs:simpleType>
  <xs:simpleType name="booleanKlType">
    <xs:annotation>
      <xs:documentation>Kewill specific boolean type using ^ as true and ` as false.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[\^`]"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CAType">
    <xs:annotation>
      <xs:documentation>Boolean type to specify a true or false value</xs:documentation>
    </xs:annotation>
    <xs:union memberTypes="booleanType booleanKlType"/>
  </xs:simpleType>
  <xs:simpleType name="S3Type">
    <xs:annotation>
      <xs:documentation>Decimal value (maximum number of fractional digits 3).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="N2Type">
    <xs:annotation>
      <xs:documentation>Non-negative decimal value (maximum number of fractional digits 2).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="N3Type">
    <xs:annotation>
      <xs:documentation>Non-negative decimal value (maximum number of fractional digits 3).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="3"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="S2Type">
    <xs:annotation>
      <xs:documentation>Decimal value (maximum number of fractional digits 2).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="N6Type">
    <xs:annotation>
      <xs:documentation>Non-negative decimal value (maximum number of fractional digits 6).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="6"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediCustomerNumberType">
    <xs:annotation>
      <xs:documentation>The edi settings are located,combined with the other ediParm values, with this number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="NType">
      <xs:maxInclusive value="999999999999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediCustomerDepartmentType">
    <xs:restriction base="AType">
      <xs:maxLength value="4"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediParm1Type">
    <xs:annotation>
      <xs:documentation>This controls, in combination with ediParm2 and ediParm3, the processing of a file. With these settings Kewill Logistics can determine departments/defaults etc.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="NType">
      <xs:maxInclusive value="9"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediParm2Type">
    <xs:restriction base="AType">
      <xs:maxLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediParm3Type">
    <xs:restriction base="AType">
      <xs:maxLength value="1"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="transmitterType">
    <xs:restriction base="AType">
      <xs:maxLength value="35"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="receiverType">
    <xs:restriction base="AType">
      <xs:maxLength value="35"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediReferenceType">
    <xs:annotation>
      <xs:documentation>This is used to identify edi files.It is used when updates/ delete/ add is send of a previous transmission.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="35"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="referenceIndicationType">
    <xs:annotation>
      <xs:documentation>Indication of the reference which is refered to when using the ediReference field. not set or set with 0  the ediReference is an external EDI reference set with 1  the ediReference points is the con note number set with 9  the ediReference points to a message reference</xs:documentation>
    </xs:annotation>
    <xs:restriction base="NType">
      <xs:enumeration value="0"/>
      <xs:enumeration value="1"/>
      <xs:enumeration value="9"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="internalShipmentNumberType">
    <xs:annotation>
      <xs:documentation>This will update the correct file directly without using the ediReference.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="NType">
      <xs:maxInclusive value="999999999999999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediFunction1Type">
    <xs:annotation>
      <xs:documentation>1-Remove transmission 2-Items to be added 4-Replace goodsLine(s) with new Line 5-Replace transmission 6-Confirmation transmission 9-Initial transmission</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ediCustomerSearchNameType">
    <xs:restriction base="AType">
      <xs:maxLength value="20"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="reasonType">
    <xs:restriction base="AType">
      <xs:maxLength value="40"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="employeeInitialsType">
    <xs:annotation>
      <xs:documentation>The initial of the person who created/updated/deleted this EDI file.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="4"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="dateTimeType">
    <xs:annotation>
      <xs:documentation>This is the actual date/time/zone value.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="DTType"/>
  </xs:simpleType>
  <xs:simpleType name="operationalPeriodType">
    <xs:annotation>
      <xs:documentation>Period number</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="7"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="arrivalDateType">
    <xs:restriction base="DYType"/>
  </xs:simpleType>
  <xs:simpleType name="loadingDateType">
    <xs:restriction base="DYType"/>
  </xs:simpleType>
  <xs:simpleType name="loadingTimeType">
    <xs:restriction base="TSType"/>
  </xs:simpleType>
  <xs:simpleType name="unloadingDateType">
    <xs:restriction base="DYType"/>
  </xs:simpleType>
  <xs:simpleType name="unloadingTimeType">
    <xs:annotation>
      <xs:documentation>This is the actual time of unloading</xs:documentation>
    </xs:annotation>
    <xs:restriction base="TSType"/>
  </xs:simpleType>
  <xs:simpleType name="marksAndNumberType">
    <xs:annotation>
      <xs:documentation>This tag is ignored when marksAndNumber in goodsLine or goodsLine/ marksAndNumbers is used.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="16"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="waybillType">
    <xs:annotation>
      <xs:documentation>External master freight number</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="17"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="houseWaybillType">
    <xs:annotation>
      <xs:documentation>External freight number</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="17"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="primaryReferenceType">
    <xs:annotation>
      <xs:documentation>This reference is the reference you can search for. For example your order number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="30"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="serviceLevelType">
    <xs:annotation>
      <xs:documentation>Depending on configuration. See program ts2126</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="4"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tariffTypeType">
    <xs:annotation>
      <xs:documentation>This can determine a further specication of a tariff. This can be for example dangerous goods, full trailer load, assembly required at unloading address. See program ts2112 for possible values.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="deliveryTermType">
    <xs:annotation>
      <xs:documentation>Possible values can be found in program ts2109.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="deliveryTermPlaceType">
    <xs:restriction base="AType">
      <xs:maxLength value="15"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="originPlaceCodeType">
    <xs:annotation>
      <xs:documentation>The place of origin coded. Preferable the UN/LOCODE is used. See program ts2124 for possible values.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AUType">
      <xs:maxLength value="5"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="destinationPlaceCodeType">
    <xs:annotation>
      <xs:documentation>The place of destination coded. Preferable the UN/LOCODE is used. See program ts2124 for possible values.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AUType">
      <xs:maxLength value="5"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="codeShedHandlingType">
    <xs:annotation>
      <xs:documentation>Informational only. True when the goods are handled in a transport shed (HUB/ DEPOT). False when the goods are picked and delivered in the same trip.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="CAType"/>
  </xs:simpleType>
  <xs:simpleType name="daysOfShedRentType">
    <xs:restriction base="NType">
      <xs:maxInclusive value="999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="shedLocationBerthType">
    <xs:restriction base="AUType">
      <xs:maxLength value="6"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="quantityType">
    <xs:restriction base="S3Type">
      <xs:minInclusive value="-9999999999.999"/>
      <xs:maxInclusive value="9999999999.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="packageCodeType">
    <xs:annotation>
      <xs:documentation>The package code of the items.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="commodityCodeType">
    <xs:annotation>
      <xs:documentation>This code is the collection code for goods. Example: ELEC. This can be used for goods like washing machine, wash dryer, dish washer,TV.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="4"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="commodityDescriptionType">
    <xs:annotation>
      <xs:documentation>This tag is used when the commodityCode is not known by the sender. Kewill Logistics will try to identify the correct commodityCode via pre-defined codes/ search values. This description is used when this can't be found.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AType">
      <xs:maxLength value="20"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="grossWeightType">
    <xs:annotation>
      <xs:documentation>Weight incl. packaging</xs:documentation>
    </xs:annotation>
    <xs:restriction base="S3Type">
      <xs:minInclusive value="-9999999999.999"/>
      <xs:maxInclusive value="9999999999.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="netWeightType">
