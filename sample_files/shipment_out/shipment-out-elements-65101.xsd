    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CollectiveCMRType">
    <xs:restriction base="CAType"/>
  </xs:simpleType>
  <xs:simpleType name="containerPlanningType">
    <xs:restriction base="CAType"/>
  </xs:simpleType>
  <xs:simpleType name="finalDestinationPlaceCodeType">
