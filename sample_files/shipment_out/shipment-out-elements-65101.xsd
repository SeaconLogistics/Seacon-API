      <xs:documentation>Weight without packaging.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="S3Type">
      <xs:minInclusive value="-9999999999.999"/>
      <xs:maxInclusive value="9999999999.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="chargeableWeightType">
    <xs:restriction base="S3Type">
      <xs:minInclusive value="-9999999999.999"/>
      <xs:maxInclusive value="9999999999.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="volumeCodeType">
    <xs:annotation>
      <xs:documentation>C=Cubical L=Loading meters</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AUType">
      <xs:enumeration value="C"/>
      <xs:enumeration value="L"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="volumeType">
    <xs:annotation>
      <xs:documentation>This is the volume. When the volume code is C this is HxWxL . When the volume code is L this is WxL.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="S3Type">
      <xs:minInclusive value="-9999999999.999"/>
      <xs:maxInclusive value="9999999999.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="cubicMetersType">
    <xs:restriction base="S3Type">
      <xs:minInclusive value="-9999999999.999"/>
      <xs:maxInclusive value="9999999999.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="loadingMetersType">
    <xs:restriction base="S3Type">
      <xs:minInclusive value="-9999999999.999"/>
      <xs:maxInclusive value="9999999999.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="goodsValueCurrencyType">
    <xs:restriction base="AUType">
      <xs:maxLength value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="goodsValueType">
    <xs:restriction base="N2Type">
      <xs:maxInclusive value="999999999999.99"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="lengthType">
    <xs:restriction base="N3Type">
      <xs:maxInclusive value="99.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="widthType">
    <xs:restriction base="N3Type">
      <xs:maxInclusive value="99.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="heightType">
    <xs:restriction base="N3Type">
      <xs:maxInclusive value="99.999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="deliveryTermAgentType">
    <xs:restriction base="AType">
      <xs:maxLength value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="domesticTransportType">
    <xs:annotation>
      <xs:documentation>Y=Yes N=No O=Own</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AUType">
      <xs:enumeration value="Y"/>
      <xs:enumeration value="N"/>
      <xs:enumeration value="O"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="domesticTransportAmountType">
    <xs:restriction base="S2Type">
